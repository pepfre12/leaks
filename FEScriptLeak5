coroutine.resume(coroutine.create(function()
Shattered:FindFirstChildOfClass("Humanoid").Health = Shattered:FindFirstChildOfClass("Humanoid").Health - 1.5
end))
rsiz = math.random(5,15)
rval = rval + 100
for i = 0, 2 do
end
shakes(0.01,0.1)
end
Shattered:FindFirstChildOfClass("Humanoid").Health = Shattered:FindFirstChildOfClass("Humanoid").Health == 0
Shattered:FindFirstChildOfClass("Humanoid").RootPart.Anchored = false
end
end
attack = false
	hum.WalkSpeed = storehumanoidWS
	end
function MILK()
attack = true
local targetted = nil
if mouse.Target.Parent ~= Character and mouse.Target.Parent.Parent ~= Character and mouse.Target.Parent:FindFirstChildOfClass("Humanoid") ~= nil then
targetted = mouse.Target.Parent
end
if targetted == nil then
	attack = false
end
local targetsHEAD = targetted.Head
if targetted ~= nil then


turnto()
bosschatfunc("And u died to mULK",Color3.new(255,255,255),2)
ragdoll(targetted)
MakeDebuff(targetsHEAD,"Debuff: MILK",Color3.new(255,255,255),Color3.new(0,0,0))
coroutine.resume(coroutine.create(function()
for i = 0, 10, 0.1 do
  	swait()
  	local Alpha = .3
  	RootJoint.C0 = RootJoint.C0:lerp(cf(0,0,0)*angles(math.rad(0),math.rad(0),math.rad(0))*RootCF,Alpha)
  	LH.C0 = LH.C0:lerp(cf(-1,0.4,0)*angles(math.rad(0),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(-90),math.rad(0)),Alpha)
  	RH.C0 = RH.C0:lerp(cf(1,0.4,0)*angles(math.rad(0),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(90),math.rad(0)),Alpha)
  	LW.C0 = LW.C0:lerp(cf(0.3,0.5,0)*angles(math.rad(0),math.rad(0),math.rad(-90)),Alpha)
  	RW.C0 = RW.C0:lerp(cf(-0.5,0.5,0)*angles(math.rad(0),math.rad(0),math.rad(90)),Alpha)
  	Torso.Neck.C0 = Torso.Neck.C0:lerp(cf(0,-1.1,0)*angles(math.rad(0),math.rad(0),math.rad(0))*necko,Alpha)
end
end))
for _,v2 in pairs(targetted:GetChildren()) do
if v2:isA("BasePart") then
	local FireEffectEM=  MILKEffect:Clone()
	FireEffectEM.Parent=v2	
	coroutine.resume(coroutine.create(function()
	local ortans=	v2.Transparency	


for i = 0,1,0.005 do
if v2.Parent~=nil then
	v2.Transparency = ortans + i
wait()	

	
end	
	
	
	
end	
	
for i = 0,1,0.01 do
if FireEffectEM.Parent~=nil then
	FireEffectEM.Transparency = NumberSequence.new(.1+i,1)
		FireEffectEM.Rate = 75-i*100
wait()	

	
end	
	
	
	
end			
	wait(.5)
	
	v2:Destroy()	
	end))	
	
	
		end
		end	
	
	
	





Powery = Powery - 560
attack = false
end
end

function ScytheAttack1()
	attack = true
	hum.WalkSpeed = 0
  for i = 0, 2, 0.1 do
  	swait()
  	local Alpha = .3
  	RootJoint.C0 = RootJoint.C0:lerp(cf(0,0,0)*angles(math.rad(0),math.rad(20),math.rad(0))*RootCF,Alpha)
  	LH.C0 = LH.C0:lerp(cf(-1,-1,0)*angles(math.rad(0),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(-90),math.rad(0)),Alpha)
  	RH.C0 = RH.C0:lerp(cf(1,-1,0)*angles(math.rad(0),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(90),math.rad(0)),Alpha)
  	LW.C0 = LW.C0:lerp(cf(-1.3,0.3,0.6)*angles(math.rad(-167.5),math.rad(80),math.rad(-107.3)),Alpha)
  	RW.C0 = RW.C0:lerp(cf(1.6,0.4,0.1)*angles(math.rad(85.5),math.rad(3),math.rad(-9.6)),Alpha)
  	Torso.Neck.C0 = Torso.Neck.C0:lerp(cf(0,0,0)*angles(math.rad(0),math.rad(0),math.rad(0))*necko,Alpha)
  end
MagniDamage2(ScytheWEEW.HitBox,10,30,50,1,"Knockdown")
--CFuncs["EchoSound"].Create("rbxassetid://260430117", root, 4, 1,0,10,0.15,0.5,1)
shakes(0.5,1)
  for i = 0, 3, 0.1 do
  	swait()
  	local Alpha = .3
  	RootJoint.C0 = RootJoint.C0:lerp(cf(-0.1,0,-0.1)*angles(math.rad(0),math.rad(-20),math.rad(0))*RootCF,Alpha)
  	LH.C0 = LH.C0:lerp(cf(-1,-1,0)*angles(math.rad(0),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(-90),math.rad(0)),Alpha)
  	RH.C0 = RH.C0:lerp(cf(1,-1,0)*angles(math.rad(0),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(90),math.rad(0)),Alpha)
  	LW.C0 = LW.C0:lerp(cf(-1.5,0.5,-0.1)*angles(math.rad(-85),math.rad(87.8),math.rad(80)),Alpha)
  	RW.C0 = RW.C0:lerp(cf(1.3,0.5,0)*angles(math.rad(85),math.rad(2.1),math.rad(10.4)),Alpha)
  	Torso.Neck.C0 = Torso.Neck.C0:lerp(cf(0,0,0)*angles(math.rad(0),math.rad(0),math.rad(0))*necko,Alpha)
  end
attack = false
hum.WalkSpeed = storehumanoidWS
end

function CreateMeshskidglitcher(parent, meshtype, x1, y1, z1)
	local mesh = Instance.new("SpecialMesh", parent)
	mesh.MeshType = meshtype
	mesh.Scale = Vector3.new(x1 * 10, y1 * 10, z1 * 10)
	return mesh
end
