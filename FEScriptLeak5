local GlitchScript = script.Glitch:Clone()

local static1 = {{"479939522"},{"426859113"},{"446601018"},{"477780809"},{"2432665152"}}

local GlitchSounds = {{"1342917734"},{"458294126"},{"169112309"}}

local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local ImageLabel = Instance.new("ImageLabel")
ScreenGui.Parent = nil
Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.new(1, 1, 1)
Frame.BackgroundTransparency = 1
Frame.BorderSizePixel = 3
Frame.Size = UDim2.new(1, 0, 1, 0)
ImageLabel.Parent = Frame
ImageLabel.BackgroundColor3 = Color3.new(1, 1, 1)
ImageLabel.BackgroundTransparency = 1
ImageLabel.Size = UDim2.new(1, 0, 1, 0)
ImageLabel.ImageTransparency = 0.7

function Glitch3(PlayerSelect,Trans,TSays)
local glitchs = GlitchScript:Clone()

glitchs.Trans.Value = Trans
glitchs.TSays.Value = TSays
glitchs.Disabled = false
glitchs.Parent = PlayerSelect
game:GetService("Debris"):AddItem(glitchs, 60)	
end

function Glitch2()
	coroutine.resume(coroutine.create(function()	
local StaticScreen = ScreenGui:Clone()	
StaticScreen.Parent = player.PlayerGui	


local StaticImage = StaticScreen.Frame.ImageLabel
	local LifeTime =  math.random(5,30)	
LifeTime=LifeTime/100
game:GetService("Debris"):AddItem(StaticScreen, LifeTime*10)		

for i = 3,0,-0.01 do
	wait()
local ggg = static1[math.random(1,#static1)]
StaticImage.Image = 'rbxassetid://'..ggg[1]
StaticImage.Rotation =  math.random(-10,10)/10	
StaticImage.ImageTransparency = StaticImage.ImageTransparency +.03

end
end))
end


function TargetKill()
	local targetted
	if mouse.Target.Parent ~= Character and mouse.Target.Parent.Parent ~= Character and mouse.Target.Parent:FindFirstChildOfClass("Humanoid") ~= nil then
		targetted = mouse.Target.Parent
	end
if targetted ~= nil then
attack = true
Glitch2()
local originalpos = root.CFrame
RootPart.CFrame = targetted.Head.CFrame * CFrame.new(0,-2,2)
targetted.Head.Anchored = true
        for i = 0, 9 do
        sphereMK(3,0.25,"Add",root.CFrame*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),1,1,10,-0.01,MAINRUINCOLOR,MAINRUINCOLOR.Color,0)
        end
        for i = 0, 24 do
        PixelBlock(1,math.random(4,8),"Add",root.CFrame*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),3,3,3,0.06,MAINRUINCOLOR,0)
        end
for i = 0,5,0.1 do
swait()
  	local Alpha = .3
  	RootJoint.C0 = RootJoint.C0:lerp(cf(0,0,0)*angles(math.rad(0),math.rad(37),math.rad(0))*RootCF,Alpha)
  	LH.C0 = LH.C0:lerp(cf(-1.1,-0.9,0)*angles(math.rad(-10.1),math.rad(18.3),math.rad(-10.1))*angles(math.rad(0),math.rad(-90),math.rad(0)),Alpha)
  	RH.C0 = RH.C0:lerp(cf(0.9,-1,0.2)*angles(math.rad(0),math.rad(-28.3),math.rad(3.2))*angles(math.rad(0),math.rad(90),math.rad(0)),Alpha)
  	LW.C0 = LW.C0:lerp(cf(-1.7,0.5,0.2)*angles(math.rad(84),math.rad(3.8),math.rad(47.4)),Alpha)
  	RW.C0 = RW.C0:lerp(cf(1.5,0.7,0.1)*angles(math.rad(-22.1),math.rad(-14.1),math.rad(31)),Alpha)
  	Torso.Neck.C0 = Torso.Neck.C0:lerp(cf(0,0,0)*angles(math.rad(0),math.rad(-34.4),math.rad(0))*necko,Alpha)
end
CFuncs["Sound"].Create("rbxassetid://429400881", root, 3, 1)
for i = 0, 12 do
sphere2(1,"Add",targetted.Head.CFrame*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),vt(1,1,1),-0.01,10,-0.01,BrickColor.new("Really blue"),BrickColor.new("Really blue").Color)
end
for i = 0,1,0.1 do
swait()
  	local Alpha = .3
  	RootJoint.C0 = RootJoint.C0:lerp(cf(-0.1,0,0.3)*angles(math.rad(0),math.rad(-59.5),math.rad(0))*RootCF,Alpha)
  	LH.C0 = LH.C0:lerp(cf(-1,-1,0.2)*angles(math.rad(-5.3),math.rad(39.6),math.rad(-3.7))*angles(math.rad(0),math.rad(-90),math.rad(0)),Alpha)
  	RH.C0 = RH.C0:lerp(cf(0.9,-1,0.2)*angles(math.rad(0),math.rad(-28.3),math.rad(3.2))*angles(math.rad(0),math.rad(90),math.rad(0)),Alpha)
  	LW.C0 = LW.C0:lerp(cf(-1.6,0.5,-0.2)*angles(math.rad(92.4),math.rad(1.4),math.rad(-59.7)),Alpha)
  	RW.C0 = RW.C0:lerp(cf(1.5,0.6,0.1)*angles(math.rad(-11),math.rad(-12.7),math.rad(9.1)),Alpha)
  	Torso.Neck.C0 = Torso.Neck.C0:lerp(cf(0,0,0)*angles(math.rad(0),math.rad(56.5),math.rad(0))*necko,Alpha)
end
dmg(targetted)
for i = 0,10,0.1 do
swait()
  	local Alpha = .3
  	RootJoint.C0 = RootJoint.C0:lerp(cf(-0.1,0,0.3)*angles(math.rad(0),math.rad(-59.5),math.rad(0))*RootCF,Alpha)
  	LH.C0 = LH.C0:lerp(cf(-1,-1,0.2)*angles(math.rad(-5.3),math.rad(39.6),math.rad(-3.7))*angles(math.rad(0),math.rad(-90),math.rad(0)),Alpha)
  	RH.C0 = RH.C0:lerp(cf(0.9,-1,0.2)*angles(math.rad(0),math.rad(-28.3),math.rad(3.2))*angles(math.rad(0),math.rad(90),math.rad(0)),Alpha)
  	LW.C0 = LW.C0:lerp(cf(-1.6,0.5,-0.2)*angles(math.rad(92.4),math.rad(1.4),math.rad(-59.7)),Alpha)
  	RW.C0 = RW.C0:lerp(cf(1.5,0.6,0.1)*angles(math.rad(-11),math.rad(-12.7),math.rad(9.1)),Alpha)
  	Torso.Neck.C0 = Torso.Neck.C0:lerp(cf(0,0,0)*angles(math.rad(0),math.rad(56.5),math.rad(0))*necko,Alpha)
end
root.CFrame = originalpos
        for i = 0, 9 do
        sphereMK(3,0.25,"Add",root.CFrame*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),1,1,10,-0.01,MAINRUINCOLOR,MAINRUINCOLOR.Color,0)
        end
        for i = 0, 24 do
        PixelBlock(1,math.random(4,8),"Add",root.CFrame*CFrame.Angles(math.rad(math.random(-360,360)),math.rad(math.random(-360,360)),math.rad(math.random(-360,360))),3,3,3,0.06,MAINRUINCOLOR,0)
        end
attack = false
hum.WalkSpeed = storehumanoidWS
elseif targetted == nil then
Glitch3(Character,.2,"I can't attack the air...")	
chatfunc("I can't attack the air...")
end
end

