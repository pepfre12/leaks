  	Torso.Neck.C0 = Torso.Neck.C0:lerp(cf(0,0.2,0.5)*angles(math.rad(-26.8),math.rad(0),math.rad(0))*necko,Alpha)
	end
		for i = 0, 10, 0.1 do
		swait()
  	local Alpha = .3
  	RootJoint.C0 = RootJoint.C0:lerp(cf(0,-0.3,-0.1)*angles(math.rad(-35.1),math.rad(0),math.rad(0))*RootCF,Alpha)
