--UNDERTALESOUL1.Transparency = 0
--wait(2)
--UNDERTALESOUL1.Transparency = 1
--UNDERTALESOUL_broke.Transparency = 0
wait(2)
script:remove()
--Dust(char,true)
print("...")

	elseif ModeOfGlitch == "Musicy." then
			spawn(function()
			local parts = char:GetChildren()
			for l = 1, #parts do
				if parts[l].ClassName == "MeshPart" or parts[l].ClassName == "Part" then
					parts[l].CanCollide = false
					parts[l].Anchored = true
					
					spawn(function()
						repeat wait()
							parts[l].Transparency = parts[l].Transparency + 0.1
						until parts[l].Transparency >= 1
					end)
					
					if parts[l].ClassName == "Accessory" then
						spawn(function()
							repeat wait()
								parts[l].Handle.Transparency = parts[l].Handle.Transparency + 0.1
							until parts[l].Handle.Transparency >= 1
						end)
					end
				end
			end
			end)
			
			
--			HeartCloneMusicy.BrickColor = BrickColor.new("Black")
--			HeartCloneMusicy.Parent = char
			HeartCloneMusicy.CFrame = Torso.CFrame
			HeartCloneMusicy.Orientation = Vector3.new(0,0,180)
			HeartCloneMusicy.Transparency = 0
			coroutine.resume(coroutine.create(function()
			while true do
			swait(0)
			HeartCloneMusicy.Color = Color3.new(r / 255, g / 255, b / 255)
			end
			end))
			wait(0.1)
			
			local OrigianlHeartPos = HeartCloneMusicy.CFrame
			for i = 1,10,0.1 do
				wait()
				HeartCloneMusicy.CFrame = CFrame.new(OrigianlHeartPos.X+math.random(-1,1)/10,OrigianlHeartPos.Y+math.random(-1,1)/10,OrigianlHeartPos.Z+math.random(-1,1)/10)
			end
			HeartCloneMusicy:Destroy()
			
			
			BrokenHeartCloneMusicy.Orientation = Vector3.new(0,0,180)
--			BrokenHeartCloneMusicy.Parent = char
			BrokenHeartCloneMusicy.CFrame = OrigianlHeartPos
			BrokenHeartCloneMusicy.Transparency = 0
			BrokenHeartCloneMusicy.Separate:Play()
			coroutine.resume(coroutine.create(function()
			while true do
			swait(0)
			BrokenHeartCloneMusicy.Color = Color3.new(r / 255, g / 255, b / 255)
			end
			end))
			wait(1.2)
			BrokenHeartCloneMusicy.Transparency = 1
		
			
			for i = 1,math.random(6,8) do
				local newpart = Instance.new("Part", char)
				newpart.Size = Vector3.new(math.random(15,25)/100,math.random(15,25)/100,math.random(15,25)/100)
				newpart.CFrame = BrokenHeartCloneMusicy.CFrame
				newpart.Color = BrokenHeartCloneMusicy.Color
				newpart.Material = BrokenHeartCloneMusicy.Material
				newpart.CanCollide = false
				
				newpart.Velocity = Vector3.new(math.random(-15,15),math.random(-5,25),math.random(-15,15))
				newpart.RotVelocity = Vector3.new(math.random(-10,10),math.random(-10,10),math.random(-10,10))
				
				local BodyForce = Instance.new("BodyForce", newpart)
				BodyForce.Force = Vector3.new(0,newpart:GetMass()*170,0)
			end
wait(4)
Dust(char,true)
	end
end

function PunchAtt1()
attack = true
hum.WalkSpeed = 0
local soundi = Instance.new("Sound",workspace)
soundi.SoundId = "rbxassetid://2854929194"
soundi.Volume = 4
soundi:Play()
  for i = 0, 1, 0.1 do
  	swait()
  	local Alpha = .3
  	RootJoint.C0 = RootJoint.C0:lerp(cf(-0.1,0,-0.3)*angles(math.rad(0),math.rad(2.8),math.rad(0))*RootCF,Alpha)
  	LH.C0 = LH.C0:lerp(cf(-1,-1,0.1)*angles(math.rad(1.1),math.rad(13.3),math.rad(-4.7))*angles(math.rad(0),math.rad(-90),math.rad(0)),Alpha)
  	RH.C0 = RH.C0:lerp(cf(0.8,-1.3,-0.9)*angles(math.rad(-41.2),math.rad(11.3),math.rad(8.9))*angles(math.rad(0),math.rad(90),math.rad(0)),Alpha)
  	LW.C0 = LW.C0:lerp(cf(-1.2,-0.1,1.1)*angles(math.rad(116.2),math.rad(-5.8),math.rad(-20.6)),Alpha)
  	RW.C0 = RW.C0:lerp(cf(1,0.2,0.5)*angles(math.rad(-65),math.rad(-30.4),math.rad(-47.3)),Alpha)
  	Torso.Neck.C0 = Torso.Neck.C0:lerp(cf(-0.3,0.2,0.3)*angles(math.rad(-26.3),math.rad(-16.4),math.rad(-13.7))*necko,Alpha)
  end
CFuncs["Sound"].Create("rbxassetid://153092213", Head, 2.5,1)
local hitb = CreateParta(m,1,1,"SmoothPlastic",BrickColor.Random())
hitb.Anchored = true
hitb.CFrame = root.CFrame + root.CFrame.lookVector*4
MagniDamage(hitb, 4, 10,50, 0, "Knockdown",153092213)
hitb:Destroy()
  for i = 0, 2, 0.1 do
