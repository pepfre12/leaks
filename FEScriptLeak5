  	RootJoint.C0 = RootJoint.C0:lerp(cf(0,0 + 0.1 * math.cos(sine / 20),0)*angles(math.rad(0),math.rad(0),math.rad(0))*RootCF,Alpha)
  	LH.C0 = LH.C0:lerp(cf(-1,-1 - 0.1 * math.cos(sine / 20),0)*angles(math.rad(0),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(-90),math.rad(0)),Alpha)
  	RH.C0 = RH.C0:lerp(cf(1,-1.1 - 0.1 * math.cos(sine / 20),-0.3)*angles(math.rad(-20),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(90),math.rad(0)),Alpha)
