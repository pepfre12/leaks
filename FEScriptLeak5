local ScytheWEEW = script.Gauntlets.Scythe1
--ScytheWEEW.Parent = Character
local finalweld2 = Instance.new("Weld",ScytheWEEW.Weld)
finalweld2.Part0 = ScytheWEEW.Weld
finalweld2.Part1 = rarm
ScytheWEEW.Weld.Anchored = false

function Scythetra(trans)
for i,v in pairs (ScytheWEEW:GetChildren()) do
if v.Name ~= "Weld" and v.Name ~= "Welder" and v.Name ~= "HitBox" then
	v.Transparency = trans
end
end
end
Scythetra(1)
local gauntlet = script.Gauntlets.Gauntlet
gauntlet.Parent = char
local finalweld = Instance.new("Weld",gauntlet.Weld)
finalweld.Part0 = gauntlet.Weld
finalweld.Part1 = larm
finalweld.C0 = CFrame.new(0,0,0)*CFrame.Angles(math.rad(0),math.rad(180),math.rad(0))
gauntlet.Weld.Anchored = false

