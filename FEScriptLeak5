v.Color = one
end
end
for i, v in pairs(gunty:GetChildren()) do
if v:IsA("Part") then
v.Color = one
v.Material = "Neon"
end
end
for i, v in pairs(gunty2:GetChildren()) do
if v:IsA("Part") then
v.Color = two
v.Material = "Neon"
end
end
