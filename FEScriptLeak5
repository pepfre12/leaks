--gauntlet2.Weld.Anchored = false

function RecolorThingBC(one,two,three,four,five,exonetran,exone,extwotran,extwo)

--lolcolorBC(,one.Color,0.50) 
for i,v in pairs(gauntlet:GetChildren()) do
if v.Name == "Neon" then
v.BrickColor = two
end
end
--for i,v in pairs(gauntlet2:GetChildren()) do
--if v.Name == "Neon" then
--v.BrickColor = one
--end
--end
for i, v in pairs(mw2:GetChildren()) do
	if v:IsA("UnionOperation") then
v.Trail.Color = ColorSequence.new(one.Color,one.Color)
v.Trail2.Color = ColorSequence.new(one.Color,one.Color)
end
end
for i, v in pairs(mw1:GetChildren()) do
	if v:IsA("UnionOperation") then
v.Trail.Color = ColorSequence.new(two.Color,two.Color)
v.Trail2.Color = ColorSequence.new(two.Color,two.Color)
end
end
for i, v in pairs(m:GetChildren()) do
	if v:IsA("UnionOperation") then
v.Trail.Color = ColorSequence.new(two.Color,two.Color)
v.Trail2.Color = ColorSequence.new(two.Color,two.Color)
end
end
for i, v in pairs(m2:GetChildren()) do
	if v:IsA("UnionOperation") then
v.Trail.Color = ColorSequence.new(two.Color,two.Color)
v.Trail2.Color = ColorSequence.new(two.Color,two.Color)
end
end
for i, v in pairs(m3:GetChildren()) do
	if v:IsA("UnionOperation") then
v.Trail.Color = ColorSequence.new(two.Color,two.Color)
v.Trail2.Color = ColorSequence.new(two.Color,two.Color)
end
end
for i,v in pairs(Corer:GetChildren())do
if v.Name ~= "CORE" and v.Name ~= "COREtwo" then
v.BrickColor = one
end
end
for i, v in pairs(gunty:GetChildren()) do
if v:IsA("Part") then
v.BrickColor = one
v.Material = "Neon"
end
end
for i, v in pairs(gunty2:GetChildren()) do
if v:IsA("Part") then
v.BrickColor = two
v.Material = "Neon"
end
end
for i, v in pairs(mw4:GetChildren()) do
if v:IsA("Part") then
v.BrickColor = one
v.Material = "Neon"
end
end
for i, v in pairs(mw5:GetChildren()) do
if v:IsA("Part") then
v.BrickColor = two
v.Material = "Neon"
end
end
for i, v in pairs(mw2:GetChildren()) do
if v:IsA("UnionOperation") then
v.BrickColor = one
v.Material = "Neon"
end
end
for i, v in pairs(mw1:GetChildren()) do
if v:IsA("UnionOperation") then
v.Transparency = 0
v.BrickColor = two
v.Material = "Neon"
end
end
for i, v in pairs(m:GetChildren()) do
if v:IsA("UnionOperation") then
v.BrickColor = three
v.Material = "Ice"
end
end
for i, v in pairs(m2:GetChildren()) do
if v:IsA("UnionOperation") then
v.BrickColor = four
v.Material = "Ice"
end
end
for i, v in pairs(m3:GetChildren()) do
if v:IsA("UnionOperation") then
v.BrickColor = five
v.Material = "Neon"
end
end
for i, v in pairs(extrawingmod1:GetChildren()) do
if v:IsA("UnionOperation") then
v.Transparency = exonetran
v.BrickColor = exone
v.Material = "Neon"
end
end
for i, v in pairs(extrawingmod2:GetChildren()) do
if v:IsA("UnionOperation") then
v.Transparency = extwotran
v.BrickColor = extwo
v.Material = "Neon"
end
end
MAINRUINCOLORhahah2 = false
corea.BrickColor = one
coreb.BrickColor = two
end
function RecolorThingC3(one,two,three,four,five,exonetran,exone,extwotran,extwo)
for i,v in pairs(gauntlet:GetChildren()) do
if v.Name == "Neon" then
v.Color = two
end
end
--for i,v in pairs(gauntlet2:GetChildren()) do
--if v.Name == "Neon" then
--v.Color = one
--end
