  	RH.C0 = RH.C0:lerp(cf(1,-1.3,0)*angles(math.rad(19.8),math.rad(7.9),math.rad(-1.8))*angles(math.rad(0),math.rad(90),math.rad(0)),Alpha)
  	LW.C0 = LW.C0:lerp(cf(-1.7,0.4,-0.4)*angles(math.rad(116.2),math.rad(-5.8),math.rad(-20.6)),Alpha)
  	RW.C0 = RW.C0:lerp(cf(1,0.2,0.5)*angles(math.rad(-65),math.rad(-30.4),math.rad(-47.3)),Alpha)
  	Torso.Neck.C0 = Torso.Neck.C0:lerp(cf(0.2,0.1,-0.3)*angles(math.rad(-3.4),math.rad(40.4),math.rad(11.9))*necko,Alpha)
  end
