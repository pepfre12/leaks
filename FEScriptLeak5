bossison = false
function BossTime()
attack = true
bossison = true
char.HumanoidRootPart.Anchored = true
  for i = 0, 1, 0.1 do
  	swait()
  	local Alpha = .3
  	RootJoint.C0 = RootJoint.C0:lerp(cf(0,-0.2,0.8)*angles(math.rad(9.8),math.rad(0),math.rad(0))*RootCF,Alpha)
  	LH.C0 = LH.C0:lerp(cf(-1,-1.2,0)*angles(math.rad(45.7),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(-90),math.rad(0)),Alpha)
  	RH.C0 = RH.C0:lerp(cf(1,-1.2,-0.1)*angles(math.rad(42.1),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(90),math.rad(0)),Alpha)
  	LW.C0 = LW.C0:lerp(cf(-1.5,0.4,0)*angles(math.rad(47.5),math.rad(0),math.rad(0)),Alpha)
  	RW.C0 = RW.C0:lerp(cf(1.5,0.4,0)*angles(math.rad(50.2),math.rad(0),math.rad(0)),Alpha)
  	Torso.Neck.C0 = Torso.Neck.C0:lerp(cf(0,0,-0.3)*angles(math.rad(19.3),math.rad(0),math.rad(0))*necko,Alpha)
  end
  for i = 0, 5, 0.1 do
  	swait()
  	local Alpha = .3
  	RootJoint.C0 = RootJoint.C0:lerp(cf(0,-2.7,3.1)*angles(math.rad(89.3),math.rad(0),math.rad(0))*RootCF,Alpha)
  	LH.C0 = LH.C0:lerp(cf(-1,-1,0)*angles(math.rad(0.9),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(-90),math.rad(0)),Alpha)
  	RH.C0 = RH.C0:lerp(cf(1,-0.9,0)*angles(math.rad(-2.8),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(90),math.rad(0)),Alpha)
  	LW.C0 = LW.C0:lerp(cf(-1.6,0.6,0)*angles(math.rad(-3.6),math.rad(0),math.rad(-29.5)),Alpha)
  	RW.C0 = RW.C0:lerp(cf(1.5,0.5,0)*angles(math.rad(5.3),math.rad(0),math.rad(26.6)),Alpha)
  	Torso.Neck.C0 = Torso.Neck.C0:lerp(cf(0,-0.1,0.2)*angles(math.rad(-8.7),math.rad(0),math.rad(0))*necko,Alpha)
  end
  for i = 0, 10, 0.1 do
  	swait()
  	local Alpha = .3
  	RootJoint.C0 = RootJoint.C0:lerp(cf(0,-1.1,2.6)*angles(math.rad(0.6),math.rad(0),math.rad(0))*RootCF,Alpha)
  	LH.C0 = LH.C0:lerp(cf(-1,0,-0.6)*angles(math.rad(0.9),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(-90),math.rad(0)),Alpha)
  	RH.C0 = RH.C0:lerp(cf(1,-1.2,-0.6)*angles(math.rad(-74.5),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(90),math.rad(0)),Alpha)
  	LW.C0 = LW.C0:lerp(cf(-1.6,0.5,-0.1)*angles(math.rad(70.1),math.rad(-6.7),math.rad(28.8)),Alpha)
  	RW.C0 = RW.C0:lerp(cf(1.1,0.3,-0.7)*angles(math.rad(77.5),math.rad(6.7),math.rad(-62.2)),Alpha)
  	Torso.Neck.C0 = Torso.Neck.C0:lerp(cf(0,-0.1,0)*angles(math.rad(-2.8),math.rad(0),math.rad(0))*necko,Alpha)
  end
bosschatfunc("What just..",Color3.new(0,255,255),2)
  for i = 0, 10, 0.1 do
