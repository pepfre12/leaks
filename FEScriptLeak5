  	local Alpha = .3
  	RootJoint.C0 = RootJoint.C0:lerp(cf(-0.3,0,0.1)*angles(math.rad(0),math.rad(86.8),math.rad(0))*RootCF,Alpha)
  	LH.C0 = LH.C0:lerp(cf(-1.1,-0.9,0.3)*angles(math.rad(0.6),math.rad(19.1),math.rad(-10.7))*angles(math.rad(0),math.rad(-90),math.rad(0)),Alpha)
  	RH.C0 = RH.C0:lerp(cf(1.1,-1,0.2)*angles(math.rad(-0.9),math.rad(-22.7),math.rad(11.4))*angles(math.rad(0),math.rad(90),math.rad(0)),Alpha)
  	LW.C0 = LW.C0:lerp(cf(-1.5,0.4,0.2)*angles(math.rad(8),math.rad(23.1),math.rad(-42.6)),Alpha)
  	RW.C0 = RW.C0:lerp(cf(1.5,0.5,0.2)*angles(math.rad(86.5),math.rad(-5.9),math.rad(89.9)),Alpha)
  	Torso.Neck.C0 = Torso.Neck.C0:lerp(cf(0.1,0,0.1)*angles(math.rad(-0.3),math.rad(-51.3),math.rad(5.5))*necko,Alpha)
end
orbe.Transparency = 1
orb.Transparency = 1
orb.CFrame = root.CFrame*CFrame.new(0,0.5,0) + root.CFrame.lookVector*5
CFuncs["Sound"].Create("rbxassetid://294188875", char, 1, 1.5)
