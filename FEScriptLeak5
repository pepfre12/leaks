	youplayedyourself.Volume = 10
	youplayedyourself.Name = "i-"
	youplayedyourself:Play()
	chatfunc("WHY WON'T YOU JUST...")
  for i = 0, 10, 0.1 do
  	swait()
  	local Alpha = .05
  	RootJoint.C0 = RootJoint.C0:lerp(cf(0,0,0.2)*angles(math.rad(24.8),math.rad(0),math.rad(0))*RootCF,Alpha)
  	LH.C0 = LH.C0:lerp(cf(-1,-1,-0.1)*angles(math.rad(-26.8),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(-90),math.rad(0)),Alpha)
  	RH.C0 = RH.C0:lerp(cf(1,-0.2,-0.9)*angles(math.rad(-27.9),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(90),math.rad(0)),Alpha)
  	LW.C0 = LW.C0:lerp(cf(-1.6,0.3,0.2)*angles(math.rad(112.2),math.rad(0),math.rad(-22.8)),Alpha)
  	RW.C0 = RW.C0:lerp(cf(1.6,0.5,0)*angles(math.rad(127.5),math.rad(0),math.rad(20.5)),Alpha)
  	Torso.Neck.C0 = Torso.Neck.C0:lerp(cf(0,0,0)*angles(math.rad(3.3),math.rad(0),math.rad(0))*necko,Alpha)
  end
	chatfunc("DIE?!")
	CFuncs.Sound.Create("rbxassetid://438666141", char, 7.5, 1)
	CFuncs.Sound.Create("rbxassetid://1208650519", char, 7.5, 1)
	CameraEnshaking(3, 20)
	for i, v in pairs(FindNearestHead(Torso.CFrame.p, 1234567890)) do
		if v:FindFirstChild("Head") then
			dmg(v)
		end
	end
	sphere(5, "Add", root.CFrame * CFrame.new(0, -2.9, 0), vt(0, 0, 0), 1000, BrickColor.random())
	sphere(10, "Add", root.CFrame * CFrame.new(0, -2.9, 0), vt(0, 0, 0), 2000, BrickColor.random())
	sphere(1, "Add", root.CFrame * CFrame.new(0, -2.9, 0), vt(100000, 0.1, 100000), 0.01, BrickColor.random())
	for i = 0, 3, 0.1 do
		swait()
		sphereMK(2.5, 0.75, "Add", root.CFrame * CFrame.new(math.random(-525, 525), -5, math.random(-525, 525)) * CFrame.Angles(math.rad(90 + math.rad(math.random(-45, 45))), math.rad(math.random(-45, 45)), math.rad(math.random(-45, 45))), 2.5, 2.5, 25, -0.025, BrickColor.random(),BrickColor.Random().Color, 0)
		sphereMK(2.5, 0.75, "Add", root.CFrame * CFrame.new(math.random(-525, 525), -5, math.random(-525, 525)) * CFrame.Angles(math.rad(90 + math.rad(math.random(-45, 45))), math.rad(math.random(-45, 45)), math.rad(math.random(-45, 45))), 2.5, 2.5, 25, -0.025, BrickColor.random(),BrickColor.Random().Color, 0)
