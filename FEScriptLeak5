local attacktype = 1
mouse.Button1Down:connect(function()
	if ModeOfGlitch == "Hate" then
	if attack == false and attacktype == 1 then
		attacktype = 1
		ScytheAttack1()
	end
	else
	if attack == false and attacktype == 1 then
		attacktype = 2
		PunchAtt1()
	elseif attack == false and attacktype == 2 then
		attacktype = 3
		attack1()
	elseif attack == false and attacktype == 3 then
		attacktype = 4
		attack2()
	elseif attack == false and attacktype == 4 then
		attacktype = 1
		Attack3()
		end
	end
end)
--3494472936
SwimType = 1
--Swimtype 1 is Front Crawl.
--Swimtype 2 is Back Stroke.
--Swimtype 3 is Butterfly.
--Swimtype "Fly" will be for flying modes, And will be a fast fly through the water.
--I might add Swimtype 4, Which will be Breast Stroke.
CanGoOnModes = true
local Muted = false
mouse.KeyDown:connect(function(k)
	if k == "l" and Muted == false then
		
				bosschatfunc("Muted!",Color3.new(0,255,0),1)

		MusicOSTthingy:pause()
		Muted = true
	elseif k == "l "and Muted == true then
		Muted = false
				bosschatfunc("Unmuted!",Color3.new(255,0,0),1)
		MusicOSTthingy:Resume()
	end	
	end)
