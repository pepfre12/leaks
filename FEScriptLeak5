	  	local Alpha = .3
	  	RootJoint.C0 = RootJoint.C0:lerp(cf(0,0,0)*angles(math.rad(0),math.rad(0),math.rad(0))*RootCF,Alpha)
	  	LH.C0 = LH.C0:lerp(cf(-1,-1,0)*angles(math.rad(0),math.rad(0),math.rad(-6))*angles(math.rad(0),math.rad(-90),math.rad(0)),Alpha)
	  	RH.C0 = RH.C0:lerp(cf(0.9,-1,0)*angles(math.rad(0),math.rad(0),math.rad(12))*angles(math.rad(0),math.rad(90),math.rad(0)),Alpha)
	  	LW.C0 = LW.C0:lerp(cf(-0.8,0.3,-0.8)*angles(math.rad(12.1),math.rad(1.1),math.rad(85.7)),Alpha)
	  	RW.C0 = RW.C0:lerp(cf(0.7,0.3,-0.9)*angles(math.rad(-14),math.rad(0),math.rad(-90)),Alpha)
	  	Torso.Neck.C0 = Torso.Neck.C0:lerp(cf(0,0,0)*angles(math.rad(0),math.rad(0),math.rad(0))*necko,Alpha)
	end
	repeat
		swait()--2
		frick = frick + 1
		Lightning(RightArm.CFrame.p,orb.CFrame.p , 5, .5, BrickColor.new("Alder"), .15, 0,-.25*2,.5,nil)

		
	until frick == 50
	frick = 0

		CFuncs.Sound.Create("rbxassetid://151304356", orb, 5, 1)
		MagniDamage(orb, 65, 65, 90, 0, "PureBombeEff",BrickColor.Random())--TAG >.> Freeze
		sphere(1, "Add", orb.CFrame, vt(orbm.Scale.x, orbm.Scale.y, orbm.Scale.z), 1, BrickColor.new("Toothpaste"))
--		sphere(2, "Add", orb.CFrame, vt(orbm.Scale.x, orbm.Scale.y, orbm.Scale.z), 2, BrickColor.new("Toothpaste"))
		for i = 0, 9 do
			sphereMK(1, 2.5, "Add", orb.CFrame * CFrame.Angles(math.rad(math.random(-360, 360)), math.rad(math.random(-360, 360)), math.rad(math.random(-360, 360))), 5, 5, 50, -0.05, BrickColor.new("Toothpaste"),BrickColor.new("Toothpaste").Color, 0)
--			sphereMK(2, 5, "Add", orb.CFrame * CFrame.Angles(math.rad(math.random(-360, 360)), math.rad(math.random(-360, 360)), math.rad(math.random(-360, 360))), 5, 5, 50, -0.05, BrickColor.new("Toothpaste"),BrickColor.new("Toothpaste").Color, 0)
		end
		wait(1)

	storehumanoidWS = 16
	hum.WalkSpeed = 16
	attack = false
end

Frame_Speed2 = 1 / 30
ArtificialHB2 = Instance.new("BindableEvent", script)
ArtificialHB2.Name = "ArtificialHB2"

script:WaitForChild("ArtificialHB2")

frame2 = Frame_Speed2
tf2 = 0
allowframeloss2 = false
tossremainder2 = false
lastframe2 = tick()
script.ArtificialHB2:Fire()

game:GetService("RunService").Heartbeat:connect(function(s, p)
	tf2 = tf2 + s
	if tf2 >= frame2 then
		if allowframeloss2 then
			script.ArtificialHB2:Fire()
			lastframe2 = tick()
		else
			for i = 1, math.floor(tf2 / frame2) do
				script.ArtificialHB2:Fire()
			end
		lastframe2 = tick()
		end
		if tossremainder2 then
			tf2 = 0
		else
			tf2 = tf2 - frame2 * math.floor(tf2 / frame2)
		end
	end
end)

--//=================================\\
--\\=================================//

function Swait2(NUMBER)
	if NUMBER == 0 or NUMBER == nil then
		ArtificialHB2.Event:wait()
	else
		for i = 1, NUMBER do
			ArtificialHB2.Event:wait()
		end
	end
end
function SetTween2(SPart,CFr,MoveStyle2,outorin2,AnimTime)
local MoveStyle = Enum.EasingStyle[MoveStyle2]
local outorin = Enum.EasingDirection[outorin2]


local dahspeed=1


if SPart.Name=="Bullet" then
dahspeed=1	
end

local tweeningInformation = TweenInfo.new(
	AnimTime/dahspeed,	
	MoveStyle,
	outorin,
	0,
	false,
	0
)
local TweenService = game:GetService("TweenService")
local MoveCF = CFr
local tweenanim = TweenService:Create(SPart,tweeningInformation,MoveCF)
tweenanim:Play()
end
function Lightning(Part0,Part1,Times,Offset,Color,Thickness,Trans,SizeAdd,Time,Dely) -- Lightning module
    --[[Part0 = Vector3 (Start pos)
		Part1 = Vector3 (End pos)
		Times = number (Amount of lightning parts)
		Offset = number (Offset)
		Color = color (brickcolor value)
		Thickness = number (thickness)
		Trans = number (transparency)
    ]]--
coroutine.resume(coroutine.create(function()
    local magz = (Part0 - Part1).magnitude
    local curpos = Part0
    local trz = {-Offset,Offset}
local Time2 = 0.07
if Time ~= nil then
Time2 = Time	
end
    for i=1,Times do
        local li = Instance.new("Part", Torso)
		li.Name = "Lightning"
		li.TopSurface =0
		li.Material = "Neon"
		li.BottomSurface = 0
		li.Anchored = true
		li.Locked = true
		li.Transparency = Trans or 0
		li.BrickColor = Color
		li.formFactor = "Custom"
		li.CanCollide = false
	--	MakeForm(li,"Ball")
		li.Size = Vector3.new(0,0,magz/Times)
local Offzet = Vector3.new(trz[(math.random(1,2)*100)/100],trz[(math.random(1,2)*100)/100],trz[(math.random(1,2)*100)/100])
        local trolpos = CFrame.new(curpos,Part1)*CFrame.new(0,0,magz/Times).p+Offzet
        if Times == i then
        local magz2 = (curpos - Part1).magnitude
        li.Size = Vector3.new(0,0,magz2)
		SetTween2(li,{Size = VT(Thickness*(1-(i/Times+.1)),Thickness*(1-(i/Times+.1)),magz2),Transparency=1},"Back","Out",Time)
        
        li.CFrame = CFrame.new(curpos,Part1)*CFrame.new(0,0,-magz2/2)
        else
        li.CFrame = CFrame.new(curpos,trolpos)*CFrame.new(0,0,magz/Times/2)		
SetTween2(li,{Size = VT(Thickness*(1-(i/Times)),Thickness*(1-(i/Times)),magz/Times),Transparency=1},"Back","Out",Time)
        
        end
        curpos = li.CFrame*CFrame.new(0,0,magz/Times/2).p
coroutine.resume(coroutine.create(function()
	wait(Time)
	li:Destroy()
end))
        --game.Debris:AddItem(li,Time)

if Dely ~= nil then
	Swait2(Dely)
end


--BlockEffect(Color, li.CFrame, li.Size.X*20, li.Size.Y*20, li.Size.Z*20, SizeAdd, SizeAdd, 0, Time2,2)
    end
end))
end

function Shadow(transparency)
FHead.Transparency = transparency
print("Changed the transparency of the face shadow to "..transparency.."!")
end



