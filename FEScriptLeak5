function MakeDebuff(inwhat,text,textcolor1,textcolor2)
local DebuffMAIN = script.DEBUFFER:Clone()
local DebuffText = DebuffMAIN.TextLabel
local DebuffTextc1 = DebuffMAIN.TextLabel.TextColor3
local DebuffTextc2 = DebuffMAIN.TextLabel.TextStrokeColor3
DebuffMAIN.Enabled = true
DebuffMAIN.Parent = inwhat
DebuffTextc1 = textcolor1
DebuffTextc2 = textcolor2
DebuffText.Text = text
end
function GrabAndShoot()
						attack=true
	local model = FDless.character
								--Humanoid.JumpPower = 0
			coroutine.resume(coroutine.create(function()
				change = 0.5
    local hitray,pos = rayCast(RootPart.Position, RootPart.CFrame.lookVector, 5, model)

	local HitModel = nil
	

	if hitray == nil then
		attack = false
		bosschatfunc("I can't grab the air.",Color3.new(0,0,0),0)
end	
	if hitray ~= nil then
		print(hitray.Name)
	HitModel = hitray.Parent
						Humanoid.WalkSpeed = 0

	if hitray.Parent:isA("Accessory") or hitray.Parent:isA("Hat") then
	HitModel = hitray.Parent.Parent	
	end
	end			
				local FoundTorso = HitModel:FindFirstChild("Torso") or HitModel:FindFirstChild("UpperTorso")
				local Character2 = Player.Character
				local RightArm2 = Character2["Right Arm"]	  
				local  cn = CFrame.new

local hashum=false
	coroutine.resume(coroutine.create(function()	
	
	for _,v in pairs(HitModel:GetChildren()) do

		if v:isA("Humanoid") then
			hashum=true


--------end of amin
local HitW = Instance.new("Weld",FoundTorso) 
	HitW.C0 = cn(-.5,0,-1.0)*angles(math.rad(90),math.rad(0),math.rad(150))
	RootPart.Anchored = true	
HitW.Part0 = FoundTorso
HitW.Part1 = RightArm2

v.PlatformStand = true
			wait(1.5)
			so("http://roblox.com/asset/?id=260433450", FoundTorso, 1, 1)	
				so("http://roblox.com/asset/?id=192104941", FoundTorso, 2, 1,10)
				MakeDebuff(HitModel.Head,"Debuff: CURSED, Ragdolled, FIRE!",Color3.new(0,0,0),Color3.new(0,0,0))
for _,v2 in pairs(HitModel:GetChildren()) do
	coroutine.resume(coroutine.create(function()
		while true do
			swait()
	HitModel.Humanoid.Health = HitModel.Humanoid.Health - 0.01
		end
	end))
if v2:isA("BasePart") then
	local FireEffectEM=  UnknowEffect:Clone()
	FireEffectEM.Parent=v2	
	coroutine.resume(coroutine.create(function()
so("http://roblox.com/asset/?id=260433450", FoundTorso, 1, 1)			
	local ortans=	v2.Transparency	

		so("http://roblox.com/asset/?id=192104941", FoundTorso, 2, 1)
	wait(1.5)

for i = 0,1,0.005 do
if v2.Parent~=nil then
	v2.Transparency = ortans + i
wait()	

	
end	
	
	
	
end	


for i = 0,1,0.01 do
if FireEffectEM.Parent~=nil then
	FireEffectEM.Transparency = NumberSequence.new(.1+i,1)
		FireEffectEM.Rate = 75-i*100
		
wait()	

	
end	
	
	
	
end			
	wait(.5)
	Dust(HitModel)	
--	v2:Destroy()	
	end))	
	
	
		end
		end	
			

--			if game:GetService("Players"):GetPlayerFromCharacter(HitModel) ~= nil then
--			Glitch3(HitModel,0,radoner[math.random(1,#radoner)])	
--			end
--			Glitch3(Character,0,radoner[math.random(1,#radoner)])	


			
				wait(1.5)
--			v.Parent:BreakJoints()
--			v.Health = 0
HitW:remove()
			wait(1)
				RootPart.Anchored = false	
		end
		
	end

	end)) 
	


	--wait(.1)
	if hashum ==true then

  for i = 0, 10, 0.1 do
  	swait()
  	local Alpha = .3
  	RootJoint.C0 = RootJoint.C0:lerp(cf(0,0,0)*angles(math.rad(0),math.rad(0),math.rad(0))*RootCF,Alpha)
  	LH.C0 = LH.C0:lerp(cf(-1,-1,0)*angles(math.rad(0),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(-90),math.rad(0)),Alpha)
  	RH.C0 = RH.C0:lerp(cf(1,-1,0)*angles(math.rad(0),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(90),math.rad(0)),Alpha)
  	LW.C0 = LW.C0:lerp(cf(-1.5,0.5,0)*angles(math.rad(0),math.rad(0),math.rad(0)),Alpha)
  	RW.C0 = RW.C0:lerp(cf(1.6,0.6,-0.1)*angles(math.rad(120),math.rad(0),math.rad(10)),Alpha)
  	Torso.Neck.C0 = Torso.Neck.C0:lerp(cf(0.1,0.1,-0.4)*angles(math.rad(21.2),math.rad(-18.7),math.rad(7.1))*necko,Alpha)
  end
bosschatfunc("Im not sorry.",Color3.new(0,0,0),2)
  for i = 0, 10, 0.1 do
  	swait()
  	local Alpha = .3
  	RootJoint.C0 = RootJoint.C0:lerp(cf(0,0,0)*angles(math.rad(0),math.rad(0),math.rad(0))*RootCF,Alpha)
  	LH.C0 = LH.C0:lerp(cf(-1,-1,0)*angles(math.rad(0),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(-90),math.rad(0)),Alpha)
  	RH.C0 = RH.C0:lerp(cf(1,-1,0)*angles(math.rad(0),math.rad(0),math.rad(0))*angles(math.rad(0),math.rad(90),math.rad(0)),Alpha)
  	LW.C0 = LW.C0:lerp(cf(-1.4,0.5,-0.4)*angles(math.rad(107.5),math.rad(9.8),math.rad(48.5)),Alpha)
  	RW.C0 = RW.C0:lerp(cf(1.6,0.6,-0.1)*angles(math.rad(120),math.rad(0),math.rad(10)),Alpha)
  	Torso.Neck.C0 = Torso.Neck.C0:lerp(cf(0.1,0.1,-0.4)*angles(math.rad(21.2),math.rad(-18.7),math.rad(7.1))*necko,Alpha)
  end
Gun.FireSound:Play()
	ragdoll(HitModel)
local HitW = Instance.new("Weld",FoundTorso) 
	HitW.C0 = cn(-.5,0,-1.0)*angles(math.rad(90),math.rad(0),math.rad(150))
	--RootPart.Anchored = true	
HitW.Part0 = FoundTorso
HitW.Part1 = RightArm2
