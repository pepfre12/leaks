if radm == 1 then
bosschatfunc("HOW LONG CAN YOU TAKE THIS?",MAINRUINCOLOR.Color,1,false)
elseif radm == 2 then
bosschatfunc("SWAMP!!",MAINRUINCOLOR.Color,1,false)
elseif radm == 3 then
bosschatfunc("SCATTER!!",MAINRUINCOLOR.Color,1,false)
end
