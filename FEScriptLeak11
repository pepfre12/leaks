LW.C0=clerp(LW.C0,cf(-1.5,0.5,0)*angles(math.rad(0),math.rad(0),math.rad(-30)),.2)
end
local rot = 0
local randomrotations = math.random(1,2)
local lookv = 2.5
local power = 7
