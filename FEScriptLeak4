		waveEff(5,"Add","Out",root.CFrame*CFrame.new(0,-1,0)*CFrame.Angles(0,math.rad(math.random(-360,360)),0),vt(Increase/30,0.0005,Increase/30),Increase/90,0.1,MAINRUINCOLOR2)
