function newTheme(ID,timepos,pitch,vol)
local kanz = kan
--kanz:Stop()
kanz.Volume = vol
--kanz.TimePosition = timepos
kanz.PlaybackSpeed = pitch
kanz.Pitch = pitch
kanz.SoundId = "rbxassetid://"..ID
KanSID=ID
kanz.Name = "wrecked"
kanz.Looped = true
currentThemePlaying = kanz.SoundId
currentVol = kanz.Volume
currentPitch = kanz.Pitch
--kanz:Play()
--coroutine.resume(coroutine.create(function()
--wait(0.05)
--end))
end

function newThemeCust(ID,timepos,pitch,vol)
local kanz = kan
kanz:Stop()
kanz.Volume = vol
kanz.TimePosition = timepos
kanz.PlaybackSpeed = pitch
kanz.Pitch = pitch
kanz.SoundId = "rbxassetid://"..ID
KanSID=ID
kanz.Name = "wrecked"
kanz.Looped = true
currentThemePlaying = kanz.SoundId
currentVol = kanz.Volume
currentPitch = kanz.Pitch
kanz:Play()
coroutine.resume(coroutine.create(function()
wait(0.05)
end))
end
function ThemeAreathing(ID,timepos,pitch,parent,vol)
	local dae = kan
	dae:Stop()
	dae.Volume = vol
	dae.Pitch = pitch
	dae=ID
	dae.Name = "ohno"
	dae.Looped = true
	dae.Parent = parent
	dae:Play()
	dae.Volume = 10
	dae.PlaybackSpeed = pitch
	
end
