end
		hum.WalkSpeed = 0
	
		  for i = 0, 5, 0.1 do
  	wait()
 	local Alpha = .4
  	RootJoint.C0 = RootJoint.C0:lerp(cf(-0.1,-0.2,-0.2)*angles(math.rad(-15.4),math.rad(-27.8),math.rad(-11.1))*RootCF,Alpha)
