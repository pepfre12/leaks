  	LW.C0 = LW.C0:lerp(cf(-1.4,0.8,0)*angles(math.rad(-11),math.rad(-8.9),math.rad(-146.7)),Alpha)
  	RW.C0 = RW.C0:lerp(cf(1.6,0.6,0)*angles(math.rad(0),math.rad(0),math.rad(30)),Alpha)
